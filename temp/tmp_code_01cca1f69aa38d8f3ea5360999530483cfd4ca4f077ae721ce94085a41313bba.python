import subprocess
import sys

try:
    import pandas as pd
    print("Pandas is already installed.")
except ImportError:
    print("Pandas is not installed. Installing...")
    try:
        subprocess.check_call([sys.executable, '-m', 'pip', 'install', 'pandas', '--timeout=60'])
        print("Pandas installed successfully.")
    except subprocess.TimeoutExpired:
        print("Pandas installation timed out.")

