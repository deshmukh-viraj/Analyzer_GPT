import pandas as pd

# Load the CSV file into a pandas DataFrame
try:
    df = pd.read_csv("your_file.csv")  # Replace "your_file.csv" with the actual file name
    # Get the column names
    column_names = df.columns.tolist()

    # Print the column names
    print(column_names)
    print("Task completed: Column names have been extracted and printed.  Make sure to replace 'your_file.csv' with the correct file name.")

except FileNotFoundError:
    print("Error: The file 'your_file.csv' was not found. Please make sure the file exists and the path is correct.")
    print("Task failed: Could not extract column names due to file not found.  Make sure to replace 'your_file.csv' with the correct file name.")
except Exception as e:
    print(f"An error occurred: {e}")
    print("Task failed: Could not extract column names due to an unexpected error.  Make sure to replace 'your_file.csv' with the correct file name.")
